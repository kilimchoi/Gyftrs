<!doctype html>
<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link type="text/css" rel="stylesheet" href="stylesheets/index.css" />
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
        <script type="text/javascript" language="javascript" charset="utf-8" src="stylesheets/js/bootstrap.min.js"></script>
        <link rel="stylesheet" type="text/css" media="all" href="stylesheets/css/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" media="all" href="stylesheets/css/bootstrap-responsive.min.css">
        <link rel="stylesheet" type="text/css" media="all" href="stylesheets/css/bootstrap-responsive.css"> 
        <script type="text/javascript">

          var _gaq = _gaq || [];
          _gaq.push(['_setAccount', 'UA-43691719-1']);
          _gaq.push(['_trackPageview']);

          (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
          })();

        </script>
    </head>
    <body style="background-color:#e8e4d4">
      <header>
            <img src="images/header-logo.png" class="img-responsive" alt="Responsive image" style="margin-top: 50px; margin-bottom: 60px"/>
      </header>
      <div class="container-fluid">
        <div class="row-fluid">
            <% for(var i = 0; i < uid.length; i++) {%>
                <li style="list-style-type:none; border-bottom: solid 1px #999; border-right: solid 1px #999; box-shadow: 0 3px 10px #888;border-radius: 8px; margin-top: 20px" id="border-resize">
                  <img src="<%= pic_square[i] %>" style="border-top-left-radius: 8px">
                  <span style="position:absolute; font-family: DeliusSwashCaps-Regular; color: #C11B17; width: 93%;"><%= name[i] %></span>
                  <span style="font-family: DeliusSwashCaps-Regular; color: #C11B17; left: 62px; margin-top: 25px">Birthday: <%= bday[i] %></span>
                  <a href="/likes/<%= i%>" style="text-decoration:none; font-size: 12px value='<%= i %>'"><img src="images/selectperson.png" style="margin-top: 15px; margin-left: 40px"></img></a>
                  <br />
                </li>
            <% } %>
        </div>
      </div>
    </body>
</html>
